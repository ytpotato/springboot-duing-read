<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.duing.mapper.UserMapper">

    <select id="groupByUser" resultType="com.duing.entity.vo.GroupBean">
        SELECT COUNT(name) as cnt,age from `user` GROUP BY age
    </select>

</mapper>